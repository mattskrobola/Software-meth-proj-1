<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.LibraryController">
   <children>
      <AnchorPane prefHeight="400.0" prefWidth="300.0" AnchorPane.bottomAnchor="10.0" AnchorPane.leftAnchor="10.0" AnchorPane.rightAnchor="10.0" AnchorPane.topAnchor="10.0">
         <children>
            <ListView fx:id="listView" prefHeight="360.0" prefWidth="300.0" />
            <ButtonBar layoutY="360.0" prefHeight="40.0" prefWidth="300.0">
              <buttons>
                <Button fx:id="addButton" mnemonicParsing="false" text="Add" />
                  <Button fx:id="editButton" mnemonicParsing="false" text="Edit" />
                  <Button fx:id="deleteButton" mnemonicParsing="false" text="Delete" />
              </buttons>
            </ButtonBar>
         </children>
      </AnchorPane>
   </children>
</AnchorPane>
